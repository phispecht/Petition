<h1>Our Supporters for {{city}}</h1>
<div id="signerId">
    <div class="signer">
    <table>
        <tr><th>First Name</th><th>Last Name</th><th>Age</th><th>City</th></tr>
{{#each filteredSigners}}<tr><td>{{#if url}}<a href="{{url}}">{{/if}}{{first}}</a></td><td>{{#if url}}<a href="{{url}}">{{/if}}{{last}}</a></td><td>{{age}}</td><td><a href="/signers/{{city}}">{{city}}</a></td></tr>{{/each}}
    </table>
    </div>
</div>
